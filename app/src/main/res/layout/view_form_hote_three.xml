<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:scrollbars="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:text="@string/main.titlePersonal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="@color/colorPrimary"
        android:textSize="@dimen/main.titleregisterhotel"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:text="@string/main.asalariado"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:textColor="@color/colorPrimaryText"
        android:textSize="@dimen/main.tamanofont"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        android:weightSum="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.43"
        android:id="@+id/linearLayout">

        <TextView
            android:text="@string/main.h"
            android:layout_width="0dp"
            android:textColor="@color/colorPrimaryText"
            android:layout_height="wrap_content"
            android:layout_weight="0.2"
            android:id="@+id/textView4" />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:textAlignment="center"
            android:hint="@string/main.cero"
            android:layout_weight="0.3"
            android:id="@+id/editText8" />

        <TextView
            android:text="@string/main.m"
            android:layout_width="0dp"
            android:paddingLeft="16dp"
            android:layout_height="wrap_content"
            android:textColor="@color/colorPrimaryText"
            android:layout_weight="0.2"
            android:id="@+id/textView5" />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:textAlignment="center"
            android:hint="@string/main.cero"
            android:layout_weight="0.3"
            android:ems="10"
            android:id="@+id/editText9" />

    </LinearLayout>

    <TextView
        android:text="@string/main.independiente"
        android:textSize="@dimen/main.tamanofont"
        android:layout_width="0dp"
        android:textColor="@color/colorPrimaryText"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        android:weightSum="1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:id="@+id/linearLayout2">

        <TextView
            android:text="@string/main.h"
            android:layout_width="0dp"
            android:textColor="@color/colorPrimaryText"
            android:layout_height="wrap_content"
            android:layout_weight="0.2" />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:textAlignment="center"
            android:hint="@string/main.cero"
            android:layout_weight="0.3" />

        <TextView
            android:text="@string/main.m"
            android:layout_width="0dp"
            android:paddingLeft="16dp"
            android:textColor="@color/colorPrimaryText"
            android:layout_height="wrap_content"
            android:layout_weight="0.2" />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:hint="@string/main.cero"
            android:textAlignment="center"
            android:layout_weight="0.3"
            android:ems="10" />

    </LinearLayout>

    <TextView
        android:text="@string/main.personaeventual"
        android:textSize="@dimen/main.tamanofont"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/colorPrimaryText"
        android:id="@+id/textView7"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:weightSum="1"
        tools:ignore="MissingConstraints"
        android:id="@+id/linearLayout4"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.46"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9">

        <TextView
            android:text="@string/main.h"
            android:layout_width="0dp"
            android:textColor="@color/colorPrimaryText"
            android:layout_height="wrap_content"
            android:layout_weight="0.2" />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:textAlignment="center"
            android:ems="10"
            android:hint="@string/main.cero"
            android:layout_weight="0.3" />

        <TextView
            android:text="@string/main.m"
            android:layout_width="0dp"
            android:paddingLeft="16dp"
            android:textColor="@color/colorPrimaryText"
            android:layout_height="wrap_content"
            android:layout_weight="0.2" />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:hint="@string/main.cero"
            android:layout_weight="0.3"
            android:textAlignment="center"
            android:ems="10" />

    </LinearLayout>

    <TextView
        android:text="@string/main.trabajadorfamiliar"
        android:textSize="@dimen/main.tamanofont"
        android:layout_width="0dp"
        android:textColor="@color/colorPrimaryText"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:weightSum="1"
        tools:ignore="MissingConstraints"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        android:id="@+id/linearLayout3"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.15">

        <TextView
            android:text="@string/main.h"
            android:layout_width="0dp"
            android:textColor="@color/colorPrimaryText"
            android:layout_height="wrap_content"
            android:layout_weight="0.2" />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:textAlignment="center"
            android:ems="10"
            android:hint="@string/main.cero"
            android:layout_weight="0.3" />

        <TextView
            android:text="@string/main.m"
            android:layout_width="0dp"
            android:paddingLeft="16dp"
            android:textColor="@color/colorPrimaryText"
            android:layout_height="wrap_content"
            android:layout_weight="0.2" />

        <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:hint="@string/main.cero"
            android:layout_weight="0.3"
            android:textAlignment="center"
            android:ems="10" />

    </LinearLayout>


</android.support.constraint.ConstraintLayout>